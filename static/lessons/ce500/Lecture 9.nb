Cell[BoxData[{
 RowBox[{"Clear", "[", "myFibonacci", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"myFibonacci", "[", 
   RowBox[{"n_", "?", "NumericQ"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"previous1", "=", "1"}], ",", 
      RowBox[{"previous2", "=", "0"}], ",", "next"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "2"}], ",", 
       RowBox[{"i", "<=", "n"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"next", "=", 
         RowBox[{"previous1", "+", "previous2"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"previous2", " ", "=", " ", "previous1"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"previous1", "=", "next"}]}]}], "\[IndentingNewLine]", "]"}], 
     ";", 
     RowBox[{"(*", " ", 
      RowBox[{"end", " ", "of", " ", "For"}], " ", "*)"}], 
     "\[IndentingNewLine]", "previous1"}]}], "\[IndentingNewLine]", 
   "]"}]}]}], "Input",
