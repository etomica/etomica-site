Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "myFun", "]"}], "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
    RowBox[{
    "This", " ", "If", " ", "statement", " ", "be", " ", "written", " ", 
     "all", " ", "on", " ", "one", " ", "line"}], ";", " ", 
    RowBox[{
    "the", " ", "linebreaks", " ", "are", " ", "used", " ", "to", " ", "make",
      " ", "it", " ", "more", " ", "readable"}]}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"myFun", "[", 
   RowBox[{"x_", ",", "y_", ",", "z_"}], "]"}], ":=", 
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"z", " ", ">", " ", "y"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Print", "[", "\"\<z is greater than y\>\"", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<Here's the value of z: \>\"", ",", " ", "z"}], "]"}]}], 
    ",", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{"end", " ", "of", " ", "true"}], "-", 
      RowBox[{"result", " ", "code"}]}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{"Print", "[", 
     RowBox[{"\"\<Here's the value of x: \>\"", ",", " ", "x"}], "]"}]}], 
   "  ", 
   RowBox[{"(*", 
    RowBox[{"false", "-", 
     RowBox[{"result", " ", "code"}]}], " ", "*)"}], "\[IndentingNewLine]", 
